  "description": "AI-native browser automation engine for OpenClaw and AI agents - Job automation, price monitoring, trading bots, web scraping",
  "main": "index.js",
  "scripts": {
    "test": "node test.js",
    "test:actions": "node test-actions.js",
    "test:complete": "node test-complete.js",
