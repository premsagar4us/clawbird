{
  "name": "clawbird",
  "version": "1.0.0",
  "description": "AI-native browser automation engine for OpenClaw and AI agents - Job automation, price monitoring, trading bots, web scraping",
  "main": "index.js",
  "scripts": {
    "test": "node test.js",
    "test:actions": "node test-actions.js",
    "test:complete": "node test-complete.js",
    "test:phase3": "node test-phase3.js",
    "test:all": "npm test && npm run test:actions && npm run test:complete && npm run test:phase3",
    "dev": "node index.js",
    "workflow": "node workflows/workflow-runner.js",
    "workflow:price": "node workflows/price-monitor.js",
    "workflow:news": "node workflows/news-aggregator.js",
    "workflow:screenshots": "node workflows/screenshot-documenter.js",
    "workflow:forms": "node workflows/form-filler.js",
    "lint": "eslint *.js workflows/*.js || true"
